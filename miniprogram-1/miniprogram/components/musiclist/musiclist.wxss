/* components/musiclist/musiclist.wxss */
.musiclist-container{
  width: 220rpx;
  position: relative;
  padding-bottom: 20rpx;
}
.musiclist-img{
  width: 100%;
  height: 220rpx;
  border-radius: 10rps;
}
.musiclist-playcount{
  font-size: 24rpx;
  color: #ffffff;
  text-shadow: 1px 0 0 rgba(0, 0, 0, 0.15);
  position: absolute;
  right: 10rpx;
  top: 4rpx;
  padding-left: 26rpx;
  background: url() no-repeat 8rpx/22rpx 28rpx;
}
.musiclist-name{
  font-size: 26rpx;
  line-height: 1.2;
  padding: 4 rpx 0 0 12rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
} 